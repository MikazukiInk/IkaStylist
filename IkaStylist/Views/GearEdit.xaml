                        <DataTemplate>
                            <Image Width="45">
                                <Image.Style>
                                    <Style>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="なし">
                                                <Setter Property="Image.Source" Value="{StaticResource IconNone}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="攻撃力アップ">
