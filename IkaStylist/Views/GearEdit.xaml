                                    <Style>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding MainPower}" Value="なし">
                                                <Setter Property="Image.Source" Value="{StaticResource IconNone}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="攻撃力アップ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconDamageUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="防御力アップ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconDefenseUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="インク効率アップ(メイン)">
                                                <Setter Property="Image.Source" Value="{StaticResource IconInkSaverMain}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="インク効率アップ(サブ)">
                                                <Setter Property="Image.Source" Value="{StaticResource IconInkSaverSub}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="インク回復力アップ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconInkRecoveryUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="ヒト移動速度アップ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconRunSpeedUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="イカダッシュ速度アップ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconSwimSpeedUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="スペシャル増加量アップ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconSpecialChargeUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="スペシャル時間延長">
                                                <Setter Property="Image.Source" Value="{StaticResource IconSpecialDurationUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="復活時間短縮">
                                                <Setter Property="Image.Source" Value="{StaticResource IconQuickRespawn}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="スペシャル減少量ダウン">
                                                <Setter Property="Image.Source" Value="{StaticResource IconSpecialSaver}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="スーパージャンプ時間短縮">
                                                <Setter Property="Image.Source" Value="{StaticResource IconQuickSuperJump}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="ボム飛距離アップ">
