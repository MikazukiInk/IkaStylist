                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="ラストスパート">
                                                <Setter Property="Image.Source" Value="{StaticResource IconLastDitchEffort}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="逆境強化">
                                                <Setter Property="Image.Source" Value="{StaticResource IconTenacity}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="カムバック">
                                                <Setter Property="Image.Source" Value="{StaticResource IconComeback}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="マーキングガード">
                                                <Setter Property="Image.Source" Value="{StaticResource IconColdBlooded}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="イカニンジャ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconNinjaSquid}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="うらみ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconHaunt}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="スタートレーダー">
                                                <Setter Property="Image.Source" Value="{StaticResource IconRecon}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="ボムサーチ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconBombSniffer}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="安全シューズ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconInkResistanceUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="ステルスジャンプ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconStealthJump}"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Image.Style>
                            </Image>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>

