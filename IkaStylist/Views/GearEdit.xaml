                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="インク効率アップ(サブ)">
                                                <Setter Property="Image.Source" Value="{StaticResource IconInkSaverSub}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="インク回復力アップ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconInkRecoveryUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="ヒト移動速度アップ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconRunSpeedUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="イカダッシュ速度アップ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconSwimSpeedUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="スペシャル増加量アップ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconSpecialChargeUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="スペシャル時間延長">
                                                <Setter Property="Image.Source" Value="{StaticResource IconSpecialDurationUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="復活時間短縮">
                                                <Setter Property="Image.Source" Value="{StaticResource IconQuickRespawn}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="スペシャル減少量ダウン">
                                                <Setter Property="Image.Source" Value="{StaticResource IconSpecialSaver}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="スーパージャンプ時間短縮">
                                                <Setter Property="Image.Source" Value="{StaticResource IconQuickSuperJump}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="ボム飛距離アップ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconBombRangeUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower.Name}" Value="スタートダッシュ">
