                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="逆境強化">
                                                <Setter Property="Image.Source" Value="{StaticResource IconTenacity}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="カムバック">
                                                <Setter Property="Image.Source" Value="{StaticResource IconComeback}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="マーキングガード">
                                                <Setter Property="Image.Source" Value="{StaticResource IconColdBlooded}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="イカニンジャ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconNinjaSquid}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="うらみ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconHaunt}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="スタートレーダー">
                                                <Setter Property="Image.Source" Value="{StaticResource IconRecon}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="ボムサーチ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconBombSniffer}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="安全シューズ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconInkResistanceUp}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding MainPower}" Value="ステルスジャンプ">
                                                <Setter Property="Image.Source" Value="{StaticResource IconStealthJump}"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Image.Style>
                            </Image>
